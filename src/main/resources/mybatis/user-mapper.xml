<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="user">
    <select id="loginUser" parameterType="userDto" resultType="userDto">
        SELECT *
        FROM W_USER
        WHERE "usID" = #{usId}
          AND "usPW" = #{usPw}
    </select>
</mapper>