<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <typeAliases>
    	<typeAlias type="com.dto.ProjectDto" alias="projectDto"/>
    	<typeAlias type="com.dto.UserDto" alias="userDto"/>
        <typeAlias type="com.dto.AbilityDto" alias="abilityDto"/>
        <typeAlias type="com.dto.ChargeHistoryDto" alias="chargeHistoryDto"/>
        <typeAlias type="com.dto.MessageDto" alias="messageDto"/>
        <typeAlias type="com.commons.Criteria" alias="criteria"/>
        <typeAlias type="com.dto.ReportDto" alias="reportDto"/>
        <typeAlias type="com.dto.FinishDealDto" alias="finishDealDto"/>
        <typeAlias type="com.dto.ReviewDto" alias="reviewDto"/>
        <typeAlias type="com.dto.DealStatusDto" alias="dealDto"/>
    </typeAliases>

    <mappers>
    	<mapper resource="/mybatis/project-mapper.xml"/>
        <mapper resource="mybatis/user-mapper.xml"/>
        <mapper resource="mybatis/ability-mapper.xml"/>
        <mapper resource="mybatis/mypage-mapper.xml"/>
        <mapper resource="mybatis/mes-mapper.xml"/>
        <mapper resource="mybatis/admin-mapper.xml"/>
        <mapper resource="mybatis/report-mapper.xml"/>
        <mapper resource="mybatis/deal-mapper.xml"/>
    </mappers>


</configuration>

